<div class="container">

<div class="page-header">
  <h1>The New York Times, Cut Up</h1>
  <br>
  <p>All headlines from the Times home page, cut up.</p>
  <p>More information:</p>
  <p><a href="http://www.writing.upenn.edu/~afilreis/88v/tzara.html">http://www.writing.upenn.edu/~afilreis/88v/tzara.html</a></p>
  <p><a href="http://www.nytimes.com">http://www.nytimes.com</a></p>
    <h2>Updated once an hour.</h2>
</div>

<% headlines.forEach(function(headline) { %>
	
	<h2><%= headline.createdAt %></h2>
    
    <p><%= headline.headline %></p>

<% }) %>
</div>